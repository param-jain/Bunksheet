{
  "expo": {
    "name": "Bunksheet-Library",
    "description": "This project is really great. Please contact Team BunkSheet to implement this project in your College upon completion.",
    "slug": "bunksheet-library",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "android": {
      "package": "com.BunkSheet.BunkSheetLibrary"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}